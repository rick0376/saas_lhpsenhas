generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Password {
  id              Int      @id @default(autoincrement())
  originalName    String
  generatedPassword String
  passwordLength  Int
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  @@map("passwords")
}
